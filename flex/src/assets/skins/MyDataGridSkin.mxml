<spark:DataGridSkin xmlns:fx="http://ns.adobe.com/mxml/2009" xmlns:spark="spark.skins.spark.*"
                    xmlns:s="library://ns.adobe.com/flex/spark">
    <fx:Declarations>
        <fx:Component id="columnSeparator">
            <s:Line>
                <s:stroke>
                    <s:SolidColorStroke color="0xAAAAAA" weight="1" caps="square"/>
                </s:stroke>
            </s:Line>
        </fx:Component>

        <fx:Component id="rowSeparator">
            <s:Line>
                <s:stroke>
                    <s:SolidColorStroke color="0xAAAAAA" weight="1" caps="square"/>
                </s:stroke>
            </s:Line>
        </fx:Component>
    </fx:Declarations>
</spark:DataGridSkin>
